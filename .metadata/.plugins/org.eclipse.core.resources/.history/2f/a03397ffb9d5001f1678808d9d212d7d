package curso_programacao;

import java.util.Locale;

public class Main {

	public static void main(String[] args) {
		
		
		double z = 10.35789;
		System.out.println(z);
		System.out.printf("%.3f\n",z);
		Locale.setDefault(Locale.US);
		System.out.printf("%.3f\n",z);
		
		System.out.println("RESULTADO = " + z + "metros");

	}

}